<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="org.beangle.ems.system.WebModule" />
	<bean class="org.beangle.ems.security.WebModule"></bean>
	<bean class="org.beangle.ems.dev.WebModule"></bean>
	
	<bean name="staticfile.download" scope="prototype"
		class="org.beangle.ems.system.action.StaticfileAction" autowire="byName">
		<property name="loaders">
			<list>
				<ref bean="classpathDocLoader"></ref>
			</list>
		</property>
	</bean>

</beans>
