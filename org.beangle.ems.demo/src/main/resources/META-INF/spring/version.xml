<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="systemVersion" class="org.beangle.util.meta.SystemVersionBean">
		<property name="name" value="Beangle Demo"/>
		<property name="version" value="2.0.2-SNAPSHOT"/>
		<property name="majorVersion" value="2"/>
		<property name="minorVersion" value="0"/>
		<property name="vendor" value="Beangle Software"/>
	</bean>
	
</beans>